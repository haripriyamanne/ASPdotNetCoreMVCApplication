<div class="text-center">
    <h1>@ViewData["Heading"]</h1>
    <h3>Using View Bag</h3>
    <ol>
        @foreach (string items in ViewBag.LibraryItems)
        {
            <li>
                @items
            </li>
        }

    </ol>
</div>
<h1>Using View Data</h1>
<ul>
    @foreach (string student in ViewData["Student"] as List<string>)
    {
        <li>@student</li>
    }
</ul>
<h1>Using Temp Data</h1>
<ul>
    @foreach (var student in TempData["Students"] as List<string>)
    {
        <li>@student</li>
    }
</ul>

